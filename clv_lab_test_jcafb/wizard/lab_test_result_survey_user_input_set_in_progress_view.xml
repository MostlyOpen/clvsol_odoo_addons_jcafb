<odoo>

    <record id="clv_lab_test_result_survey_user_input_set_in_progress_view_form" model="ir.ui.view">
        <field name="name">clv.lab_test.result.survey_user_input_set_in_progress.form</field>
        <field name="model">clv.lab_test.result.survey_user_input_set_in_progress</field>
        <field name="arch" type="xml">
            <form>
                <div class="oe_right">
                </div>
                <label for="lab_test_result_ids"/>
                <field name="lab_test_result_ids"/>
                <group>
                </group>
                <footer>
                    <button 
                        type="object"
                        name="do_lab_test_result_survey_user_input_set_in_progress"
                        string="Survey User Input Set In Progress"
                        class="oe_highlight"
                        />
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>

        </field>
    </record>

    <!-- More button Action -->
    <record id="clv_lab_test_result_survey_user_input_set_in_progress_action" model="ir.actions.act_window">
        <field name="name">Survey User Input Set In Progress</field>
        <field name="binding_model_id" ref="model_clv_lab_test_result"/>
        <field name="binding_view_types">form</field>
        <field name="res_model">clv.lab_test.result.survey_user_input_set_in_progress</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>
